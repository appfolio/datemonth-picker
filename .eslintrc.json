{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
     "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "forOf": true,
            "jsx": true,
            "es6": true,
            "experimentalObjectRestSpread" : true
        }
    },
    "rules": {
        "comma-dangle": 0
    }
}
